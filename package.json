  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@prisma/client": "^4.9.0",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.7.1",
    "prisma": "^4.9.0",
    "sqlite3": "^5.1.5",
    "tweetnacl": "^1.0.3",
    "youtube-dl-exec": "^2.2.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/sqlite3": "^3.1.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
